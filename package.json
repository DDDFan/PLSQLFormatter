{
    "name": "formatter-plsql",
    "displayName": "Format plsql Tieto-Style",
    "description": "This formatter works on a selection of pl/sql code and formats it by following criteria:",
    "version": "0.9.2",
    "publisher": "BigB",
    "engines": {
        "vscode": "^1.6.0"
    },
    "categories": [
        "Formatters"
    ],
    "activationEvents": [
        "onLanguage:plsql"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "languages": [
            {
                "id": "plsql",
                "aliases": [
                    "PL/SQL"
                ],
                "extensions": [
                    "sql"
                ]
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install && tsc"
    },
    "devDependencies": {
        "@types/node": "*",
        "typescript": "2.0.6",
        "vscode": "^1.1.30"
    }
}
